<% layout('login_page') %>

<h2><%= t('.sign_up', :default => "Sign up") %></h2>

<%= bootstrap_form_for(resource, :as => resource_name, :url => registration_path(resource_name),:html => {:class => 'form-signin'}) do |f| %>
<h2 class="form-signin-heading">Feedback para Aplicações Web</h2>
  <%= devise_error_messages! %>

  <div>
  <%= f.email_field :email %></div>

  <div>
  <%= f.text_field :username %></div>

  <div>
  <%= f.password_field :password %></div>

  <div>
  <%= f.password_field :password_confirmation %></div>

  <div><%= f.submit t('.sign_up', :default => "Sign up") %></div>
  <%= render "devise/shared/links" %>
<% end %>



<div class="logo_serpro_50_anos"></div>
